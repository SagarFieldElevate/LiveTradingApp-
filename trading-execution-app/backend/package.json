{
  "name": "trading-execution-backend",
  "version": "1.0.0",
  "description": "Live trading execution backend",
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "clean": "rm -rf dist",
    "prebuild": "npm run clean",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:integration": "jest --testPathPattern=integration",
    "test:smoke": "jest --testPathPattern=smoke",
    "test:load": "jest --testPathPattern=performance",
    "lint": "eslint src --ext .ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "docker:build": "docker build -t trading-backend .",
    "docker:run": "docker run -p 3000:3000 --env-file .env trading-backend"
  },
  "dependencies": {
    "express": "^4.18.2",
    "@pinecone-database/pinecone": "^2.0.1",
    "openai": "^4.24.0",
    "axios": "^1.6.0",
    "@slack/web-api": "^6.10.0",
    "socket.io": "^4.6.0",
    "dotenv": "^16.3.1",
    "winston": "^3.11.0",
    "zod": "^3.22.0",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "ws": "^8.16.0",
    "uuid": "^9.0.1",
    "prom-client": "^15.1.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.0",
    "@types/ws": "^8.5.10",
    "@types/cors": "^2.8.17",
    "@types/uuid": "^9.0.7",
    "@types/jest": "^29.5.11",
    "@types/supertest": "^6.0.2",
    "typescript": "^5.3.3",
    "tsx": "^4.6.2",
    "eslint": "^8.55.0",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "supertest": "^6.3.3",
    "autocannon": "^7.14.0"
  }
} 